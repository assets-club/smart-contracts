{
  "name": "@assets-club/smart-contracts",
  "version": "0.0.3",
  "description": "The Assets Club smart contracts ethers.js v5 definition package.",
  "keywords": [
    "nft",
    "assets",
    "assets-club",
    "art",
    "erc721",
    "ethereum",
    "freemint",
    "smart-contract",
    "solidity",
    "ethers",
    "ethers-v5",
    "hardhat"
  ],
  "homepage": "https://theassets.club",
  "bugs": "https://github.com/assets-club/smart-contracts/issues",
  "license": "MIT",
  "maintainers": [
    "Mathieu Bour <mathieu.tin.bour@gmail.com> (https://github.com/mathieu-bour)"
  ],
  "funding": "https://theassets.club/donate",
  "files": [
    "dist/**"
  ],
  "main": "dist/index.js",
  "typings": "dist/index.d.ts",
  "repository": "github:assets-club/smart-contracts",
  "scripts": {
    "clean": "hardhat clean && rimraf dist/",
    "format": "prettier --write '**/*.{js,jsx,ts,tsx,css,less,sass,scss,json,json5,yml,yaml,md,mdx,graphql,sol}'",
    "format:check": "prettier --check '**/*.{js,jsx,ts,tsx,css,less,sass,scss,json,json5,yml,yaml,md,mdx,graphql,sol}'",
    "lint": "solhint 'contracts/**/*.sol'",
    "compile": "hardhat compile",
    "test": "hardhat test",
    "test:coverage": "hardhat coverage",
    "test:gas": "REPORT_GAS=true hardhat test",
    "deploy": "hardhat run scripts/deploy.ts",
    "build": "pnpm clean && pnpm compile && tsc -p tsconfig.package.json"
  },
  "devDependencies": {
    "@chainlink/contracts": "^0.5.1",
    "@defi-wonderland/smock": "^2.3.4",
    "@ethersproject/abi": "^5.7.0",
    "@ethersproject/abstract-provider": "^5.7.0",
    "@ethersproject/abstract-signer": "^5.7.0",
    "@ethersproject/bytes": "^5.7.0",
    "@ethersproject/providers": "^5.7.2",
    "@faker-js/faker": "^7.6.0",
    "@mathieu-bour/prettier-config": "github:mathieu-bour/prettier-config",
    "@nomicfoundation/hardhat-chai-matchers": "^1.0.4",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.6",
    "@nomicfoundation/hardhat-toolbox": "^1.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.1",
    "@nomiclabs/hardhat-etherscan": "^3.1.2",
    "@openzeppelin/contracts": "^4.8.0",
    "@typechain/ethers-v5": "^10.1.1",
    "@typechain/hardhat": "^6.1.4",
    "@types/chai": "^4.3.4",
    "@types/lodash": "^4.14.189",
    "@types/mocha": "^9.1.1",
    "@types/node": "^18.11.9",
    "chai": "^4.3.7",
    "dotenv": "^16.0.3",
    "erc721a": "^4.2.3",
    "ethers": "^5.7.2",
    "hardhat": "^2.12.2",
    "hardhat-dependency-compiler": "^1.1.3",
    "hardhat-gas-reporter": "^1.0.9",
    "lodash": "^4.17.21",
    "prettier": "^2.7.1",
    "prettier-plugin-solidity": "1.0.0-beta.24",
    "rimraf": "^3.0.2",
    "solhint": "^3.3.7",
    "solidity-bits": "^0.3.2",
    "solidity-coverage": "^0.7.22",
    "ts-node": "^10.9.1",
    "tslog": "^3.3.4",
    "typechain": "^8.1.1",
    "typescript": "^4.9.3"
  },
  "engines": {
    "node": ">=16",
    "pnpm": ">=7"
  }
}
